pipeline{
    agent any
    Stages{
        Stage('Build'){
            steps{
                  echo "build testing"
            }
        }
        Stage('Test'){
            steps{
                echo "Test testing"
            }
        }
        Stage('deploy'){
            steps{
                echo "Deploy testing"
            }
        }
        Stage('Cleanup'){
            steps{
                echo "Cleanup testing"
            }
        }
    }
}